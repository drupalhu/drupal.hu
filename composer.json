{
    "name": "drupalhu/drupalhu",
    "type": "project",
    "description": "Code base of drupal.hu.",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "sweetchuck/composer-repo-path": true,
            "sweetchuck/composer-suite": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "danielstjules/stringy": "source",
            "*": "dist"
        },
        "process-timeout": 0,
        "sort-packages": true
    },
    "repositories": {
        "nuvoleweb/drupal-behat": {
            "type": "github",
            "url": "https://github.com/albeorte96/drupal-behat.git"
        },
        "sweetchuck/dtt-core": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/dtt-core.git"
        },
        "sweetchuck/dtt-entity_legal": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/dtt-entity_legal.git"
        },
        "drupal/marvin": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/drupal-marvin.git"
        },
        "drupal/marvin_product": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/drupal-marvin_product.git"
        },
        "drupal/marvin_acquia": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/drupal-marvin_acquia.git"
        },
        "drupal/webp": {
            "type": "git",
            "url": "https://git.drupalcode.org/project/webp.git"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-yaml": "*",
        "composer/installers": "^1.6",
        "cweagans/composer-patches": "dev-main#f88fd46757d4375edf1143f0e7319a9bf407db8d",
        "drupal/acquia_connector": "^4.0",
        "drupal/acquia_search": "^3.1",
        "drupal/admin_toolbar": "^3.3",
        "drupal/aggregator": "^2.0",
        "drupal/allowed_formats": "^2.0",
        "drupal/config_split": "^1.9",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drupal/entity_legal": "^3.0@beta",
        "drupal/eu_cookie_compliance": "^1.24",
        "drupal/facets": "^3.0",
        "drupal/honeypot": "^2.1",
        "drupal/image_widget_crop": "^2.4",
        "drupal/imageapi_optimize": "^4.0",
        "drupal/media_library_edit": "^3.0",
        "drupal/metatag": "^1.22",
        "drupal/override_node_options": "^2.7",
        "drupal/pathauto": "^1.11",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/schema_metatag": "^2.4",
        "drupal/search_api_solr": "^4.2",
        "drupal/shield": "^1.7",
        "drupal/smtp": "^1.2",
        "drupal/svg_image": "^3.0",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/webp": "dev-8.x-1.x",
        "drush/drush": "^11.0",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "dmore/behat-chrome-extension": "^1.4",
        "drupal/coder": "^8.3",
        "drupal/devel": "^5.1",
        "drupal/migrate_dc": "2.x-dev",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/module_filter": "^4.0",
        "kint-php/kint": "^4.0",
        "nuvoleweb/drupal-behat": "dev-feature/Drupal-10-compatibility",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.7",
        "sweetchuck/dtt-core": "10.x-dev",
        "sweetchuck/dtt-entity_legal": "3.0.x-dev",
        "sweetchuck/robo-git": "3.x-dev",
        "sweetchuck/robo-phpcs": "3.x-dev",
        "sweetchuck/robo-phpstan": "2.x-dev",
        "sweetchuck/robo-yarn": "3.x-dev",
        "sweetchuck/utils": "1.x-dev",
        "symfony/filesystem": "^6.2",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/string": "^6.2",
        "weitzman/drupal-test-traits": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "DrupalHu\\DrupalHu\\Tests\\": "tests/src/",
            "Drush\\Commands\\app\\": "drush/Commands/custom/app/"
        }
    },
    "scripts": {
        "clean": [
            "@clean:backend",
            "@clean:frontend",
            "@clean:translations"
        ],
        "clean:backend": [
            "rm -rf bin/ reports/ vendor/",
            "rm -rf docroot/core/ docroot/modules/contrib/ docroot/profiles/contrib/ docroot/themes/contrib/ docroot/libraries/",
            "rm -rf drush/Commands/contrib/"
        ],
        "clean:frontend": [
            "rm -rf docroot/themes/custom/*/node_modules/",
            "rm -rf docroot/themes/custom/*/**/*.css",
            "rm -rf docroot/themes/custom/*/**/*.css.map"
        ],
        "clean:translations": [
            "git clean -X --force -- 'sites/all/translations/*.po'"
        ],
        "reset": [
            "@reset:solr",
            "@reset:cache",
            "@reset:files",
            "@reset:db"
        ],
        "reset:solr": [
            "drush --config=drush --yes @app.local app:search-api:index:clear || true"
        ],
        "reset:cache": [
            "drush --config=drush --yes @app.local cache:rebuild || true"
        ],
        "reset:files": [
            "rm -rf docroot/sites/*/files/*",
            "rm -rf sites/*/php_storage/twig/*",
            "rm -rf sites/*/private/*",
            "rm -rf sites/*/temporary/*"
        ],
        "reset:db": [
            "drush --config=drush --yes @app.local sql:drop"
        ],
        "site:install:prod:empty": [
            "@reset:solr",
            "@reset:cache",
            "@reset:files",
            "drush --config=drush @app.local --yes site:install appp --locale='hu' --existing-config",
            "drush --config=drush @app.local --yes config:import"
        ],
        "site:install:prod:default": [
            "@site:install:prod:empty",
            "drush --config=drush @app.local --yes pm:enable app_dc",
            "drush --config=drush @app.local --yes migrate:import --group='app_default'",
            "drush --config=drush @app.local --yes migrate:import --group='app_common'",
            "drush --config=drush @app.local --yes pm:uninstall migrate_dc,migrate_tools,migrate_plus,app_dc"
        ],
        "site:install:prod:dummy": [
            "@site:install:prod:empty",
            "drush --config=drush @app.local --yes pm:enable app_dc",
            "drush --config=drush @app.local --yes migrate:import --group='app_default'",
            "drush --config=drush @app.local --yes migrate:import --group='app_common'",
            "drush --config=drush @app.local --yes migrate:import --group='app_dummy'",
            "drush --config=drush @app.local --yes pm:uninstall migrate_dc,migrate_tools,migrate_plus,app_dc"
        ],
        "site:install:local:empty": [
            "@site:install:prod:empty",
            "drush --config=drush @app.local --yes pm:enable dblog,app_dc,devel,module_filter,views_ui"
        ],
        "site:install:local:default": [
            "@site:install:local:empty",
            "bin/drush --config=drush @app.local --yes migrate:import --group='app_default'",
            "bin/drush --config=drush @app.local --yes migrate:import --group='app_common'"
        ],
        "site:install:local:dummy": [
            "@site:install:local:empty",
            "bin/drush --config=drush @app.local --yes migrate:import --group='app_dummy'"
        ],
        "lint": [
            "@lint:shellcheck"
        ],
        "lint:shellcheck": [
            "@lint:shellcheck:circleci",
            "@lint:shellcheck:ddev",
            "@lint:shellcheck:hooks"
        ],
        "lint:shellcheck:circleci": "shellcheck $(git ls-files -- './.circleci/*.sh' './.circleci/*.bash' './.circleci/*.zsh')",
        "lint:shellcheck:ddev": "shellcheck $(git ls-files -- './.ddev/*.sh' './.ddev/*.bash' './.ddev/*.zsh')",
        "lint:shellcheck:hooks": "shellcheck $(git ls-files -- './hooks/*.sh' './hooks/*.bash' './hooks/*.zsh')",
        "suite:activate:local": [
            "rm --force composer.local.lock",
            "cp composer.lock composer.local.lock",
            "composer-suite generate",
            "COMPOSER='composer.local.json' composer update $(composer-suite changed-packages:list)"
        ],
        "suite:activate:local-migrate": [
            "rm --force composer.local-migrate.lock",
            "cp composer.lock composer.local-migrate.lock",
            "composer-suite generate",
            "COMPOSER='composer.local-migrate.json' composer update $(composer-suite changed-packages:list)"
        ]
    },
    "scripts-descriptions": {
        "clean": "<fg=yellow>Deletes</> all the third-party codes downloaded by <fg=cyan>package managers</>.",
        "clean:backend": "<fg=yellow>Deletes</> all the third-party codes downloaded by <fg=cyan>Composer</>.",
        "clean:frontend": "<fg=yellow>Deletes</> all the third-party codes downloaded by <fg=cyan>Yarn</>.",
        "clean:translations": "<fg=yellow>Deletes</> all the <fg=cyan>*.po files</> which are belong to third-party codes.",
        "reset": "<fg=red>Deletes</> all content from all data storage (database, cache, solr, public|pivate|temporary|php_storage files).",
        "reset:solr": "<fg=yellow>Deletes</> all content <fg=cyan>SearchAPI indexes</> storages.",
        "reset:cache": "<fg=yellow>Deletes</> all content from <fg=cyan>cache</> storages.",
        "reset:files": "<fg=red>Deletes</> all the uploaded <fg=cyan>files</> (public|pivate|temporary|php_storage files).",
        "reset:db": "<fg=red>Drops</> the <fg=cyan>default database</>.",
        "site:install:prod:empty": "drush site:install – without any required content (the site won't work properly).",
        "site:install:prod:default": "drush site:install – with minimum required content.",
        "site:install:prod:dummy": "drush site:install – with minimum required content and with the developer helper dummy contents.",
        "site:install:local:empty": "drush site:install – same as 'prod' but the developer helper modules are enabled.",
        "site:install:local:default": "drush site:install – same as 'prod' but the developer helper modules are enabled",
        "site:install:local:dummy": "drush site:install – same as 'prod' but the developer helper modules are enabled.",
        "lint": "@todo Create native marvin commands like 'drush marvin:lint:phpcs'."
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/libraries/slick": [
                "npm-asset/slick-carousel"
            ],
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "composer-patches": {},
        "patches": {
            "drupal/core": [
                {
                    "description": "[meta] Improve ::generateSampleValue() for common field types",
                    "url": "./patches/drupal/core/issue-2960714-string-item-sample-value-min-length.patch",
                    "extra": {
                        "issue-tracker-url": "https://www.drupal.org/project/drupal/issues/2960714"
                    }
                },
                {
                    "description": "PoStreamReader::getSeek",
                    "url": "https://www.drupal.org/files/issues/2023-02-20/drupal-po_stream_reader_seek-3126929-45.patch",
                    "extra": {
                        "issue-tracker-url": "https://www.drupal.org/project/drupal/issues/3126929"
                    }
                }
            ],
            "drupal/drupal-extension": [
                {
                    "description": "Behat config schema - extendable selectors and text",
                    "url": "https://patch-diff.githubusercontent.com/raw/jhedstrom/drupalextension/pull/640.patch",
                    "extra": {
                        "issue-tracker-url": "https://github.com/jhedstrom/drupalextension/issues/493"
                    }
                }
            ],
            "drupal/entity_legal": [
                {
                    "description": "Automated Drupal 10 compatibility fixes",
                    "url": "https://www.drupal.org/files/issues/2022-08-22/entity_legal.3.0.x-dev.rector.patch",
                    "extra": {
                        "issue-tracker-url": "https://www.drupal.org/project/entity_legal/issues/3287248"
                    }
                },
                {
                    "description": "D10 entity_storage->getQuery()->accessCheck()",
                    "url": "https://git.drupalcode.org/project/entity_legal/-/merge_requests/11.patch",
                    "extra": {
                        "issue-tracker-url": "https://www.drupal.org/project/entity_legal/issues/3341578"
                    }
                }
            ],
            "drupal/webp": [
                {
                    "description": "Drupal 10 compatibility",
                    "url": "https://www.drupal.org/files/issues/2022-08-22/webp.1.x-dev.rector.patch",
                    "extra": {
                        "issue-tracker-url": "https://www.drupal.org/project/webp/issues/3290669"
                    }
                }
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./docroot"
            },
            "file-mapping": {
                "[web-root]/modules/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/modules/README.md": {
                    "mode": "skip"
                },
                "[web-root]/modules/README.txt": {
                    "mode": "skip"
                },

                "[web-root]/profiles/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/profiles/README.md": {
                    "mode": "skip"
                },
                "[web-root]/profiles/README.txt": {
                    "mode": "skip"
                },

                "[web-root]/themes/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/themes/README.md": {
                    "mode": "skip"
                },
                "[web-root]/themes/README.txt": {
                    "mode": "skip"
                },

                "[web-root]/sites/default/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "skip"
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "skip"
                },
                "[web-root]/sites/default/example.settings.php": {
                    "mode": "skip"
                },
                "[web-root]/sites/example.settings.local.php": {
                    "mode": "skip"
                },
                "[web-root]/sites/example.sites.php": {
                    "mode": "skip"
                },
                "[web-root]/sites/development.services.yml": {
                    "mode": "skip"
                },
                "[web-root]/sites/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/sites/README.md": {
                    "mode": "skip"
                },
                "[web-root]/sites/README.txt": {
                    "mode": "skip"
                },

                "[web-root]/.csslintrc": {
                    "mode": "skip"
                },
                "[web-root]/.editorconfig": {
                    "mode": "skip"
                },
                "[web-root]/.eslintignore": {
                    "mode": "skip"
                },
                "[web-root]/.eslintrc.json": {
                    "mode": "skip"
                },
                "[web-root]/.gitattributes": {
                    "mode": "skip"
                },
                "[web-root]/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/.ht.router.php": {
                    "mode": "skip"
                },
                "[web-root]/example.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/INSTALL.md": {
                    "mode": "skip"
                },
                "[web-root]/INSTALL.txt": {
                    "mode": "skip"
                },
                "[web-root]/README.md": {
                    "mode": "skip"
                },
                "[web-root]/README.txt": {
                    "mode": "skip"
                },
                "[web-root]/.htaccess": {
                    "mode": "skip"
                },
                "[web-root]/web.config": {
                    "mode": "skip"
                },
                "[web-root]/robots.txt": {
                    "append": "sites/all/assets/robots-additions.txt"
                },
                "[project-root]/.editorconfig": {
                    "mode": "skip"
                }
            },
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        }
    }
}
